{
    "name": "edezacas/base-admin",
    "version": "2.0.0",
    "type": "symfony-bundle",
    "license": "MIT",
    "description": "Symfony 6 Base Admin Bundle with a suite of templates.",
    "authors": [
        {
            "name": "Eduard Deza",
            "email": "edezacas@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/asset": "*",
        "symfony/form": "*",
        "symfony/framework-bundle": "^6.2",
        "symfony/twig-bundle": "*"
    },
    "require-dev": {
        "symfony/debug": "*",
        "symfony/test-pack": "^1.0",
        "symfony/webpack-encore-bundle": "^2.1",
        "symfony/yaml": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "EDC\\BaseAdminBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EDC\\BaseAdminBundle\\Tests\\": "tests/"
        }
    }
}
